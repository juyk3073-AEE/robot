<?xml version="1.0"?>
<robot name="pogo">
  <link name="base_link">
    <visual>
      <geometry><cylinder radius="0.1" length="0.3"/></geometry>
      <material name="blue"><color rgba="0 0 1 1"/></material>
    </visual>
    <collision>
      <geometry><cylinder radius="0.1" length="0.3"/></geometry>
    </collision>
    <inertial>
      <mass value="5.0"/> <inertia ixx="0.01" ixy="0.0" ixz="0.0" iyy="0.01" iyz="0.0" izz="0.01"/>
    </inertial>
  </link>

  <link name="leg">
    <visual>
      <geometry><cylinder radius="0.02" length="0.5"/></geometry>
      <origin xyz="0 0 -0.25"/>
      <material name="red"><color rgba="1 0 0 1"/></material>
    </visual>
    <collision>
      <geometry><cylinder radius="0.02" length="0.5"/></geometry>
      <origin xyz="0 0 -0.25"/>
    </collision>
    <inertial>
      <mass value="1.0"/> <inertia ixx="0.001" ixy="0.0" ixz="0.0" iyy="0.001" iyz="0.0" izz="0.0001"/>
    </inertial>
  </link>

  <joint name="spring_joint" type="prismatic">
    <parent link="base_link"/>
    <child link="leg"/>
    <origin xyz="0 0 -0.15"/>
    <axis xyz="0 0 1"/> <limit lower="-0.2" upper="0.0" effort="1000" velocity="10"/> </joint>
</robot>